<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN"
    "http://java.sun.com/j2ee/dtds/web-app_2_2.dtd">

<web-app>

	<display-name>EpubCheck for Web</display-name>
    <description>Epub validator</description>

	
    <servlet>
        <servlet-name>Check</servlet-name>
        <servlet-class>com.adobe.epubcheck.web.EpubCheckServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Check</servlet-name>
        <url-pattern>Check</url-pattern>
    </servlet-mapping>
	

</web-app>
